{
  "name": "MXList",
  "version": "0.1.0",
  "summary": "对UITableView整个使用流程的一个简单封装",
  "description": "对UITableView整个使用流程的一个简单封装，屏蔽了UITableViewCell,支持直接注册任意UIView子类到UITableView\n支持对任意UIView子类重用",
  "homepage": "https://github.com/xuvw/MXList",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "xuvw": "1052110478@qq.com"
  },
  "source": {
    "git": "https://github.com/xuvw/MXList.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "subspecs": [
    {
      "name": "controller",
      "source_files": "MXList/controller/**/*.{h,m,m}",
      "frameworks": [
        "UIKit",
        "Foundation"
      ],
      "xcconfig": {
        "OTHER_LINKER_FLAGS": "-ObjC"
      }
    },
    {
      "name": "models",
      "source_files": "MXList/models/**/*.{h,m,m}",
      "frameworks": [
        "UIKit",
        "Foundation"
      ],
      "xcconfig": {
        "OTHER_LINKER_FLAGS": "-ObjC"
      }
    },
    {
      "name": "views",
      "source_files": "MXList/views/**/*.{h,m,m}",
      "frameworks": [
        "UIKit",
        "Foundation"
      ],
      "xcconfig": {
        "OTHER_LINKER_FLAGS": "-ObjC"
      }
    }
  ]
}
